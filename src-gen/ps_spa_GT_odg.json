{'id_' : 'ps_spa_GT_odg|psa131|v8|number', 'text' : '(131:8)'}
{'id_' : 'ps_spa_GT_odg|psa131|v8|text', 'text' : '¡Levántate, Señor, y ven a tu reposo, tú y el Arca de tu fuerza!'}
{'id_' : 'ps_spa_GT_odg|psa131|v9|number', 'text' : '(131:9)'}
