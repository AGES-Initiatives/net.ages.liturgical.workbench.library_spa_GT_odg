psa83.v10.text = "Mira, oh Dios, nuestro escudo, contempla la cara de tu ungido."
psa83.v11.number = "(83:11)"
psa83.v11.text = "Vale por mil un día en tus atrios, y prefiero quedarme en el umbral, delante de la casa de mi Dios antes que compartir la casa del malvado."
psa83.v11a.number = "(83:11a)"
psa83.v11a.text = "Vale por mil un día en tus atrios,"
psa83.v11b.number = "(83:11b)"
psa83.v11b.text = "y prefiero quedarme en el umbral, delante de la casa de mi Dios antes que compartir la casa del malvado."
psa83.v12.number = "(83:12)"
psa83.v12.text = "El Señor es un baluarte y un escudo, el Señor dará la gracia y la gloria a los que marchan rectamente: ninguna bendición les negará."
