psa1.title = "Salmo 1"
psa1.version = ""
psa1.text = "Dichoso el hombre que no va a reuniones de malvados, ni sigue el camino de los pecadores ni se sienta en la junta de burlones, mas le agrada la Ley del Señor y medita su Ley de noche y día. Es como árbol plantado junto al río que da fruto a su tiempo y tiene su follaje siempre verde. Todo lo que él hace le resulta. No sucede así con los impíos: son como paja llevada por el viento. No se mantendrán en el juicio los malvados ni en la junta de los justos los pecadores. Porque Dios cuida el camino de los justos y acaba con el sendero de los malos."
psa1.v1.number = "(1:1)"
psa1.v1.text = "Dichoso el hombre que no va a reuniones de malvados, ni sigue el camino de los pecadores ni se sienta en la junta de burlones,"
psa1.v2.number = "(1:2)"
psa1.v2.text = "mas le agrada la Ley del Señor y medita su Ley de noche y día."
psa1.v3.number = "(1:3)"
psa1.v3.text = "Es como árbol plantado junto al río que da fruto a su tiempo y tiene su follaje siempre verde. Todo lo que él hace le resulta."
psa1.v4.number = "(1:4)"
psa1.v4.text = "No sucede así con los impíos: son como paja llevada por el viento."
psa1.v5.number = "(1:5)"
psa1.v5.text = "No se mantendrán en el juicio los malvados ni en la junta de los justos los pecadores."
