bode9.title = "Ode Nine"
bode9.version = ""
bode9_mag.book = "Evangelio según San Lucas"
bode9_mag.heading1 = ""
bode9_mag.chapverse = "(1:46-55)"
bode9_mag.heading2 = ""
bode9_mag.intro = ""
bode9_mag.v1.number = "(9a:1)"
bode9_mag.v1.text = "Proclama mi alma la grandeza del Señor, y mi espíritu se alegra en Dios mi Salvador,"
bode9_mag.v2.number = "(9a:2)"
bode9_mag.v2.text = "porque se fijó en su humilde esclava, y desde ahora todas las generaciones me dirán feliz."
bode9_mag.v3.number = "(9a:3)"
bode9_mag.v3.text = "El Poderoso ha hecho grandes cosas por mí: ¡Santo es su Nombre! Muestra su misericordia siglo tras siglo a todos aquellos que viven en su presencia."
bode9_mag.v4.number = "(9a:4)"
bode9_mag.v4.text = "Dio un golpe con todo su poder: deshizo a los soberbios y sus planes."
bode9_mag.v5.number = "(9a:5)"
bode9_mag.v5.text = "Derribó a los poderosos de sus tronos y exaltó a los humildes. Colmó de bienes a los hambrientos, y despidió a los ricos con las manos vacías."
bode9_mag.v6.number = "(9a:6)"
bode9_mag.v6.text = "Socorrió a Israel, su siervo, se acordó de su misericordia, como lo había prometido a nuestros padres, a Abraham y a sus descendientes para siempre."

bode9.book = "Evangelio según San Lucas"
bode9.heading1 = ""
bode9.chapverse = "(1:68-79)"
bode9.heading2 = ""
bode9.intro = ""
bode9.v1.number = "(9b:1)"
bode9.v1.text = "Bendito sea el Señor, Dios de Israel, porque ha visitado y redimido a su pueblo."
bode9.v2.number = "(9b:2)"
bode9.v2.text = "Ahora sale triunfante nuestra salvación en la casa de David, su siervo,"
bode9.v3.number = "(9b:3)"
bode9.v3.text = "como lo había dicho desde tiempos antiguos por boca de sus santos profetas:"
bode9.v4.number = "(9b:4)"
bode9.v4.text = "que nos salvaría de nuestros enemigos y de la mano de todos los que nos odian;"
bode9.v5.number = "(9b:5)"
bode9.v5.text = "que nos mostraría el amor que tiene a nuestros padres y cómo recuerda su santa alianza."
bode9.v6.number = "(9b:6)"
bode9.v6.text = "Pues juró a nuestro padre Abraham que nos libraría de nuestros enemigos"
bode9.v7.number = "(9b:7)"
bode9.v7.text = "para que lo sirvamos sin temor, justos y santos, todos los días de nuestra vida."
bode9.v8.number = "(9b:8)"
bode9.v8.text = "Y tú, niño, serás llamado Profeta del Altísimo, porque irás delante del Señor para preparle sus caminos,"
bode9.v9.number = "(9b:9)"
bode9.v9.text = "para decir a su pueblo lo que será su salvación. Pues van a recibir el perdón de sus pecados, obra de la misericordia de nuestro Dios,"
bode9.v10.number = "(9b:10)"
bode9.v10.text = "cuando venga de lo alto para visitarnos cual sol naciente, iluminando a los que viven en tinieblas, sentados en la sombra de la muerte,"
bode9.v11.number = "(9b:11)"
bode9.v11.text = "y guiar nuestros pasos por un sendero de paz."
