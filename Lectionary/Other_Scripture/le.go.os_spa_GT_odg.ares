leosLI.pe.d095.CommunionHymn.title = "Communion Hymn for Mid-Pentecost."
leosLI.pe.d095.CommunionHymn.version = ""
leosLI.pe.d095.CommunionHymn.book = "Evangelio según San Juan"
leosLI.pe.d095.CommunionHymn.number = "(6:56)"
leosLI.pe.d095.CommunionHymn.text = "El que come mi carne y bebe mi sangre permanece en mí y yo en él. Así dijo el Señor."

leosTY.Beatitudes.title = "THE BEATITUDES"
leosTY.Beatitudes.version = ""
leosTY.Beatitudes.book = "Evangelio según San Mateo"
leosTY.Beatitudes.chapverse = "(5)"
leosTY.Beatitudes_Intro.text = ""
leosTY.Beatitudes_v01.text = "Felices los que tienen el espíritu del pobre, porque de ellos es el Reino de los Cielos."
leosTY.Beatitudes_v02.text = "Felices los que lloran, porque recibirán consuelo."
leosTY.Beatitudes_v03.text = "Felices los pacientes, porque recibirán la tierra en herencia."
leosTY.Beatitudes_v04.text = "Felices los que tienen hambre y sed de justicia, porque serán saciados."
leosTY.Beatitudes_v05.text = "Felices los compasivos, porque obtendrán misericordia."
leosTY.Beatitudes_v06.text = "Felices los de corazón limpio, porque verán a Dios."
leosTY.Beatitudes_v07.text = "Felices los que trabajan por la paz, porque serán reconocidos como hijos de Dios."
leosTY.Beatitudes_v08.text = "Felices los que son perseguidos por causa del bien, porque de ellos es el Reino de los Cielos."
leosTY.Beatitudes_v09.text = "Felices ustedes, cuando por causa mía los insulten, los persigan y les levanten toda clase de calumnias."
leosTY.Beatitudes_v10.text = "Alégrense y muéstrense contentos, porque será grande la recompensa que recibirán en el cielo. "
leosTY.Beatitudes_Refrain1.text = ""
leosTY.Beatitudes_Refrain2.text = ""
leosTY.Beatitudes_Refrain3.text = ""
